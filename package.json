{
  "name": "react-docgen-renderer-template",
  "version": "0.1.0",
  "description": "Allows you to create your own react-docgen based renderers",
  "main": "lib/index.js",
  "scripts": {
    "test": "lab",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/.bin/lab --coverage-exclude lib",
    "test:coverage:html": "lab -r console -o stdout -r html -o coverage.html"
  },
  "keywords": [
    "react",
    "react-docgen",
    "template",
    "render"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OriR/react-docgen-renderer-template"
  },
  "author": "Ori Riner",
  "license": "MIT",
  "devDependencies": {
    "code": "^5.2.0",
    "husky": "^1.3.1",
    "lab": "^15.5.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
